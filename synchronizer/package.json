{
    "name": "synchronizer",
    "displayName": "OpenVMS IDE synchronizer",
    "description": "",
    "version": "0.1.58",
    "publisher": "vmssoftware",
    "repository": {
        "type": "git",
        "url": "git:"
    },
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:vmssoftware.project-dep.projectDependencies",
        "onView:vmssoftware.project-dep.projectDescription",
        "onCommand:vmssoftware.synchronizer.stopSync",
        "onCommand:vmssoftware.synchronizer.buildProject",
        "onCommand:vmssoftware.synchronizer.reBuildProject",
        "onCommand:vmssoftware.synchronizer.createMMS",
        "onCommand:vmssoftware.synchronizer.cleanProject",
        "onCommand:vmssoftware.synchronizer.syncProject",
        "onCommand:vmssoftware.synchronizer.editProject",
        "onCommand:vmssoftware.synchronizer.changeCRLF",
        "onCommand:vmssoftware.synchronizer.forceSynchronized",
        "onCommand:vmssoftware.synchronizer.forceBuilt",
        "onCommand:vmssoftware.synchronizer.uploadZip",
        "onCommand:vmssoftware.synchronizer.downloadHeaders",
        "onUri"
    ],
    "main": "./out/extension",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "project-explorer",
                    "title": "Project Explorer",
                    "icon": "media/dep.svg"
                }
            ]
        },
        "views": {
            "project-explorer": [
                {
                    "id": "vmssoftware.project-dep.projectDependencies",
                    "name": "Project Dependencies"
                },
                {
                    "id": "vmssoftware.project-dep.projectDescription",
                    "name": "Project description"
                }
            ]
        },
        "menus": {
            "commandPalette": [
                {
                    "command": "vmssoftware.project-dep.projectState.didSynchronize",
                    "when": "vmssoftware.project-dep.projectState:never"
                },
                {
                    "command": "vmssoftware.project-dep.projectState.didBuild",
                    "when": "vmssoftware.project-dep.projectState:never"
                },
                {
                    "command": "vmssoftware.synchronizer.stopSync",
                    "when": "vmssoftware.synchronizer.isSync"
                },
                {
                    "command": "vmssoftware.synchronizer.buildProject",
                    "when": "!vmssoftware.synchronizer.isSync && !vmssoftware.synchronizer.isBuld"
                },
                {
                    "command": "vmssoftware.synchronizer.reBuildProject",
                    "when": "!vmssoftware.synchronizer.isSync && !vmssoftware.synchronizer.isBuld"
                },
                {
                    "command": "vmssoftware.synchronizer.cleanProject",
                    "when": "!vmssoftware.synchronizer.isSync && !vmssoftware.synchronizer.isBuld"
                },
                {
                    "command": "vmssoftware.synchronizer.syncProject",
                    "when": "!vmssoftware.synchronizer.isSync && !vmssoftware.synchronizer.isBuld"
                },
                {
                    "command": "vmssoftware.synchronizer.editProject",
                    "when": "!vmssoftware.synchronizer.isSync && !vmssoftware.synchronizer.isBuld"
                },
                {
                    "command": "vmssoftware.synchronizer.changeCRLF",
                    "when": "!vmssoftware.synchronizer.isSync && !vmssoftware.synchronizer.isBuld"
                },
                {
                    "command": "vmssoftware.synchronizer.uploadZip",
                    "when": "!vmssoftware.synchronizer.isSync && !vmssoftware.synchronizer.isBuld"
                }
            ],
            "view/title": [
                {
                    "command": "vmssoftware.project-dep.projectDependencies.refresh",
                    "when": "view == vmssoftware.project-dep.projectDependencies",
                    "group": "navigation"
                },
                {
                    "command": "vmssoftware.project-dep.projectDescription.refresh",
                    "when": "view == vmssoftware.project-dep.projectDescription",
                    "group": "navigation"
                }
            ],
            "view/item/context": [
                {
                    "command": "vmssoftware.project-dep.projectDependencies.add",
                    "when": "view == vmssoftware.project-dep.projectDependencies && viewItem == folder"
                },
                {
                    "command": "vmssoftware.project-dep.projectDependencies.build",
                    "when": "view == vmssoftware.project-dep.projectDependencies && viewItem == folder"
                },
                {
                    "command": "vmssoftware.project-dep.projectDependencies.rebuild",
                    "when": "view == vmssoftware.project-dep.projectDependencies && viewItem == folder"
                },
                {
                    "command": "vmssoftware.project-dep.projectDependencies.clean",
                    "when": "view == vmssoftware.project-dep.projectDependencies && viewItem == folder"
                },
                {
                    "command": "vmssoftware.project-dep.projectDependencies.remove",
                    "when": "view == vmssoftware.project-dep.projectDependencies && viewItem == depends"
                },
                {
                    "command": "vmssoftware.project-dep.projectDescription.edit",
                    "when": "view == vmssoftware.project-dep.projectDescription && viewItem == editable"
                }
            ]
        },
        "commands": [
            {
                "command": "vmssoftware.project-dep.projectDependencies.refresh",
                "title": "Refresh",
                "icon": {
                    "light": "resources/light/refresh.svg",
                    "dark": "resources/dark/refresh.svg"
                }
            },
            {
                "command": "vmssoftware.project-dep.projectDependencies.add",
                "title": "Add in dependencies to selected"
            },
            {
                "command": "vmssoftware.project-dep.projectDependencies.build",
                "title": "Build with dependencies"
            },
            {
                "command": "vmssoftware.project-dep.projectDependencies.rebuild",
                "title": "Rebuild with dependencies"
            },
            {
                "command": "vmssoftware.project-dep.projectDependencies.clean",
                "title": "Clean this only"
            },
            {
                "command": "vmssoftware.project-dep.projectDependencies.remove",
                "title": "Remove from dependencies"
            },
            {
                "command": "vmssoftware.project-dep.projectDescription.refresh",
                "title": "Refresh",
                "icon": {
                    "light": "resources/light/refresh.svg",
                    "dark": "resources/dark/refresh.svg"
                }
            },
            {
                "command": "vmssoftware.project-dep.projectDescription.select",
                "title": "Select"
            },
            {
                "command": "vmssoftware.project-dep.projectDescription.edit",
                "title": "Edit"
            },
            {
                "command": "vmssoftware.project-dep.projectState.didSynchronize",
                "title": "vmssoftware.project-dep.projectState.didSynchronize"
            },
            {
                "command": "vmssoftware.project-dep.projectState.didBuild",
                "title": "vmssoftware.project-dep.projectState.didBuild"
            },
            {
                "command": "vmssoftware.synchronizer.stopSync",
                "title": "%command.stopSync.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.createMMS",
                "title": "%command.createMMS.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.buildProject",
                "title": "%command.buildProject.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.reBuildProject",
                "title": "%command.reBuildProject.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.cleanProject",
                "title": "%command.cleanProject.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.syncProject",
                "title": "%command.syncProject.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.editProject",
                "title": "%command.editProject.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.changeCRLF",
                "title": "%command.changeCRLF.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.forceSynchronized",
                "title": "%command.forceSynchronized.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.forceBuilt",
                "title": "%command.forceBuilt.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.uploadZip",
                "title": "%command.uploadZip.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.downloadHeaders",
                "title": "%command.downloadHeaders.title%",
                "category": "%command.category%"
            }
        ],
        "configuration": {
            "title": "%configuration.title%",
            "properties": {
                "vmssoftware.project-dep.buildType": {
                    "scope": "resource",
                    "type": "string",
                    "enum": [
                        "DEBUG",
                        "RELEASE"
                    ],
                    "enumDescriptions": [
                        "Debug",
                        "Release"
                    ]
                },
                "vmssoftware.project-dep.dependencies": {
                    "scope": "resource",
                    "type": "array",
                    "default": [
                        {
                            "folder": "",
                            "depends": [
                                ""
                            ]
                        }
                    ],
                    "description": "Project dependencies"
                },
                "vmssoftware.synchronizer.project.break": {
                    "scope": "resource",
                    "type": "string",
                    "default": "C",
                    "description": "%properties.project.break.description%"
                },
                "vmssoftware.synchronizer.project.builders": {
                    "scope": "resource",
                    "type": "string",
                    "default": "*.{mms,com}",
                    "description": "Builders"
                },
                "vmssoftware.synchronizer.project.exclude": {
                    "scope": "resource",
                    "type": "string",
                    "default": "**/{.vscode,node-modules}",
                    "description": "%properties.project.exclude.description%"
                },
                "vmssoftware.synchronizer.project.headers": {
                    "scope": "resource",
                    "type": "string",
                    "default": "*.{h,hpp}",
                    "description": "%properties.project.headers.description%"
                },
                "vmssoftware.synchronizer.project.listing": {
                    "scope": "resource",
                    "type": "string",
                    "default": "*.lis",
                    "description": "%properties.project.listing.description%"
                },
                "vmssoftware.synchronizer.project.outdir": {
                    "scope": "resource",
                    "type": "string",
                    "default": "out",
                    "description": "%properties.project.outdir.description%"
                },
                "vmssoftware.synchronizer.project.projectName": {
                    "scope": "resource",
                    "type": "string",
                    "default": "project",
                    "description": "%properties.project.projectName.description%"
                },
                "vmssoftware.synchronizer.project.projectType": {
                    "scope": "resource",
                    "type": "string",
                    "enum": [
                        "executable",
                        "library",
                        "shareable"
                    ],
                    "enumDescriptions": [
                        "Executable",
                        "Static library",
                        "Shareable image"
                    ],
                    "default": "executable",
                    "description": "%properties.project.projectType.description%"
                },
                "vmssoftware.synchronizer.project.resource": {
                    "scope": "resource",
                    "type": "string",
                    "default": "**/resource/**/*.*",
                    "description": "%properties.project.resource.description%"
                },
                "vmssoftware.synchronizer.project.root": {
                    "scope": "resource",
                    "type": "string",
                    "default": "project",
                    "description": "%properties.project.root.description%"
                },
                "vmssoftware.synchronizer.project.source": {
                    "scope": "resource",
                    "type": "string",
                    "default": "*.{c,cpp}",
                    "description": "%properties.project.source.description%"
                },
                "vmssoftware.synchronizer.synchronize.downloadNewFiles": {
                    "scope": "resource",
                    "type": "string",
                    "enum": [
                        "overwrite",
                        "skip",
                        "edit"
                    ],
                    "enumDescriptions": [
                        "Overwrite older files",
                        "Skip files with newer version",
                        "Download and edit"
                    ],
                    "default": "edit",
                    "description": "%properties.synchronize.downloadNewFiles.description%"
                },
                "vmssoftware.synchronizer.synchronize.keepAlive": {
                    "scope": "resource",
                    "type": "boolean",
                    "default": false,
                    "description": "%properties.synchronize.keepAlive.description%"
                },
                "vmssoftware.synchronizer.synchronize.setTimeAttempts": {
                    "scope": "resource",
                    "type": "number",
                    "default": 3,
                    "description": "%properties.synchronize.setTimeAttempts.description%"
                },
                "vmssoftware.synchronizer.synchronize.setTimeByShell": {
                    "scope": "resource",
                    "type": "boolean",
                    "default": true,
                    "description": "%properties.synchronize.setTimeByShell.description%"
                },
                "vmssoftware.synchronizer.synchronize.unzipCmd": {
                    "scope": "resource",
                    "type": "string",
                    "default": "",
                    "description": "%properties.synchronize.unzipCmd.description%"
                },
                "vmssoftware.synchronizer.synchronize.preferZip": {
                    "scope": "resource",
                    "type": "boolean",
                    "default": false,
                    "description": "%properties.synchronize.preferZip.description%"
                },
                "vmssoftware.synchronizer.synchronize.purge": {
                    "scope": "resource",
                    "type": "boolean",
                    "default": false,
                    "description": "%properties.synchronize.purge.description%"
                }
            }
        }
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "get-ext-api": "PowerShell copy -Path ..\\zip-helper\\out\\zip-api.d.ts, ..\\ssh-helper\\out\\api.d.ts, ..\\ssh-helper\\out\\ssh-helper.d.ts, ..\\config-helper\\out\\config\\config.d.ts -Destination src\\ext-api\\",
        "dir": "dir ..\\ssh-helper\\out"
    },
    "devDependencies": {
        "@types/micromatch": "^3.1.0",
        "@types/mocha": "^5.2.5",
        "@types/node": "^8.10.38",
        "del": "^3.0.0",
        "event-stream": "^4.0.1",
        "gulp": "^3.9.1",
        "gulp-sourcemaps": "^2.6.4",
        "gulp-typescript": "^5.0.0-alpha.3",
        "run-sequence": "^2.2.1",
        "tslint": "^5.11.0",
        "typescript": "^3.1.6",
        "vsce": "^1.53.0",
        "vscode": "^1.1.21",
        "vscode-nls-dev": "github:vorfol/vscode-nls-dev"
    },
    "dependencies": {
        "@vorfol/common": "^0.0.20",
        "fs-extra": "^7.0.1",
        "micromatch": "^3.1.10",
        "vscode-nls": "^4.0.0"
    }
}
