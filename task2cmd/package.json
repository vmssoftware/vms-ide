{
    "name": "task2cmd",
    "displayName": "task2cmd",
    "description": "Task 2 Cmd",
    "version": "0.1.5",
    "publisher": "vmssoftware",
    "repository": {
        "type": "git",
        "url": "git:"
    },
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:workbench.action.tasks.runTask"
    ],
    "main": "./out/extension",
    "contributes": {
        "taskDefinitions": [
            {
                "type": "task2cmd",
                "required": [
                    "command"
                ],
                "properties": {
                    "command": {
                        "type": "string",
                        "description": "Command to execute"
                    },
                    "label": {
                        "type": "string",
                        "description": "Label to display"
                    },
                    "extension": {
                        "type": "string",
                        "description": "Extension which executes command"
                    },
                    "parameter": {
                        "type": "string",
                        "description": "One string parameter"
                    }
                }
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "package": "vsce package"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "tslint": "^5.11.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    },
    "dependencies": {}
}
