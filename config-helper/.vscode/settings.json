// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "out": false // set this to true to hide the "out" folder with the compiled JS files
    },
    "search.exclude": {
        "out": true // set this to false to include "out" folder in search results
    },
    // Turn off tsc task auto detection since we have the necessary tasks as npm scripts
    "typescript.tsc.autoDetect": "off",
    "vmssoftware.config-helper.using": "VSC",
    "vmssoftware.ssh-helper.connection.host": "host",
    "vmssoftware.ssh-helper.connection.port": 22,
    "vmssoftware.ssh-helper.connection.username": "",
    "vmssoftware.ssh-helper.timeouts.cmdTimeout": 0,
    "vmssoftware.ssh-helper.timeouts.feedbackTimeout": 0,
    "vmssoftware.ssh-helper.timeouts.welcomeTimeout": 0
}