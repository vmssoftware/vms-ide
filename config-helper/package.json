{
    "name": "config-helper",
    "displayName": "config-helper",
    "description": "Config helper",
    "version": "0.1.29",
    "publisher": "vmssoftware",
    "repository": {
        "type": "git",
        "url": "git:"
    },
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:vmssoftware.config-helper.test"
    ],
    "main": "./out/extension",
    "contributes": {
        "menus": {
            "commandPalette": [
                {
                    "command": "vmssoftware.config-helper.test",
                    "when": "vmssoftware.config-helper.test:enabled"
                }
            ]
        },
        "commands": [
            {
                "command": "vmssoftware.config-helper.test",
                "title": "%command.test.title%",
                "category": "%command.category%"
            }
        ],
        "configuration": {
            "title": "%configuration.title%",
            "properties": {
                "vmssoftware.config-helper.using": {
                    "type": "string",
                    "enum": [
                        "VSC",
                        "FS",
                        "VFS"
                    ],
                    "enumDescriptions": [
                        "%configuration.vsc.description%",
                        "%configuration.fs.description%",
                        "%configuration.vfs.description%"
                    ],
                    "default": "FS",
                    "description": "%configuration.type.description%"
                },
                "vmssoftware.config-helper.test": {
                    "type": "string",
                    "default": "test",
                    "description": "%configuration.test.description%"
                }
            }
        }
    },
    "scripts": {
        "package": "vsce package",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/mocha": "^5.x.x",
        "@types/node": "^8.10.37",
        "del": "^3.0.0",
        "event-stream": "^4.0.1",
        "fs-extra": "^7.0.1",
        "gulp": "^3.9.1",
        "gulp-sourcemaps": "^2.6.4",
        "gulp-typescript": "^5.0.0-alpha.3",
        "run-sequence": "^2.2.1",
        "tslint": "^5.11.0",
        "typescript": "^3.1.6",
        "vsce": "^1.53.0",
        "vscode": "^1.1.21",
        "vscode-nls-dev": "github:vorfol/vscode-nls-dev"
    },
    "dependencies": {
        "@vorfol/common": "^0.0.18",
        "vscode-nls": "^4.0.0"
    }
}
