{
    "name": "ssh-helper",
    "displayName": "ssh-helper",
    "description": "ssh-helper",
    "version": "0.1.34",
    "publisher": "vmssoftware",
    "repository": {
        "type": "git",
        "url": "git:"
    },
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:vmssoftware.ssh-helper.editSettings"
    ],
    "main": "./out/extension",
    "contributes": {
        "menus": {
            "commandPalette": [
                {
                    "command": "vmssoftware.ssh-helper.editSettings"
                }
            ]
        },
        "commands": [
            {
                "command": "vmssoftware.ssh-helper.editSettings",
                "title": "%command.edit.title%",
                "category": "%command.category%"
            }
        ],
        "configuration": {
            "title": "%configuration.title%",
            "properties": {
                "vmssoftware.ssh-helper.connection.host": {
                    "scope": "resource",
                    "type": "string",
                    "default": "host",
                    "description": "%configuration.host%"
                },
                "vmssoftware.ssh-helper.connection.port": {
                    "scope": "resource",
                    "type": "number",
                    "default": 22,
                    "description": "%configuration.port%"
                },
                "vmssoftware.ssh-helper.connection.username": {
                    "scope": "resource",
                    "type": "string",
                    "default": "",
                    "description": "%configuration.username%"
                },
                "vmssoftware.ssh-helper.connection.password": {
                    "scope": "resource",
                    "type": "string",
                    "default": "",
                    "description": "%configuration.password%"
                },
                "vmssoftware.ssh-helper.connection.keyFile": {
                    "scope": "resource",
                    "type": "string",
                    "default": "",
                    "description": "%configuration.keyFile%"
                },
                "vmssoftware.ssh-helper.timeouts.cmdTimeout": {
                    "scope": "resource",
                    "type": "number",
                    "default": 0,
                    "description": "%configuration.timeouts.command%"
                },
                "vmssoftware.ssh-helper.timeouts.feedbackTimeout": {
                    "scope": "resource",
                    "type": "number",
                    "default": 0,
                    "description": "%configuration.timeouts.feedback%"
                },
                "vmssoftware.ssh-helper.timeouts.welcomeTimeout": {
                    "scope": "resource",
                    "type": "number",
                    "default": 0,
                    "description": "%configuration.timeouts.welcome%"
                },
                "vmssoftware.ssh-helper.host-collection.hosts": {
                    "scope": "resource",
                    "type": "array",
                    "default": [
                        {
                            "label": "",
                            "host": "",
                            "port": 22,
                            "username": "",
                            "password": "",
                            "keyFile": ""
                        }
                    ],
                    "description": "array of hosts"
                }
            }
        }
    },
    "scripts": {
        "compile": "tsc",
        "package": "vsce package",
        "watch": "tsc -watch",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "get-ext-api": "PowerShell copy -Path ..\\config-helper\\out\\config\\config.d.ts -Destination src\\config\\",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.5",
        "@types/node": "^8.10.38",
        "del": "^3.0.0",
        "event-stream": "^4.0.1",
        "gulp": "^3.9.1",
        "gulp-sourcemaps": "^2.6.4",
        "gulp-typescript": "^5.0.0-alpha.3",
        "run-sequence": "^2.2.1",
        "tslint": "^5.11.0",
        "typescript": "^3.1.6",
        "vsce": "^1.53.0",
        "vscode": "^1.1.21",
        "vscode-nls-dev": "github:vorfol/vscode-nls-dev"
    },
    "dependencies": {
        "@types/ssh2": "^0.5.36",
        "@vorfol/common": "^0.0.20",
        "fs-extra": "^7.0.1",
        "ssh2": "^0.6.1",
        "vscode-nls": "^4.0.0"
    }
}
