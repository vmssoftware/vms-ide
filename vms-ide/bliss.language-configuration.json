{
    "comments": {
        "lineComment": "!",
        "blockComment": [ "%(", "%)" ]
    },
    "wordPattern": "[a-zA-Z$_][a-zA-Z$_0-9]*",
    "brackets": [
        ["(", ")"],
        ["[", "]"],
        ["<", ">"],
        ["begin", "end"]
    ],
    "autoClosingPairs": [
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"]
    ],
    "surroundingPairs": [
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"]
    ]
}