{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json"
  ,"name": "VMS-COBOL"
  ,"scopeName": "source.vms-cobol"
  , "comment": "/NOANSI_FORMAT"
  ,"patterns": [
    {
      "include": "#comment"
    }
    ,{
      "include": "#system-name"
    }
    ,{
      "include": "#identification-division"
    }
    ,{
      "include": "#environment-division"
    }
    ,{
      "include": "#data-division"
    }
    ,{
      "include": "#procedure-division"
    }
    ,{
      "include": "#end-program"
    }
    ,{
      "include": "#number"
    }
    ,{
      "include": "#string"
    }
  ]
  ,"repository": {
    "invalid-char": {
      "match": "\\S"
      ,"name": "invalid.illegal"
    }
    ,"comment": {
      "name": "comment.line"
      ,"match": "^[*/].*$"
    }
    ,"number": {
      "name": "cobol.number"
      ,"begin": "(?i)(?=\\d)|(?=(\\.|,)\\d)|(?=X[\"'])"
      ,"end": "(?!\\G)"
      ,"patterns": [
          {
            "name": "constant.numeric.float"
            ,"match": "(?i)\\G[0-9]*(\\.|,)[0-9]+(E[-+]?\\d+)"
          }
          ,{
            "name": "constant.numeric.float"
            ,"match": "(?i)\\G[0-9]+(\\.|,)[0-9]*(E[-+]?\\d+)?"
          }
          ,{
            "name": "constant.numeric.integer"
            ,"match": "(?i)\\G[0-9]+"
        }
        ,{
          "name": "constant.numeric.integer"
          ,"match": "(?i)\\GX\"[0-9a-f]+\""
        }
        ,{
          "name": "constant.numeric.integer"
          ,"match": "(?i)\\GX'[0-9a-f]+'"
        }
      ]
    }
    ,"string": {
      "begin": "(?i)(?=(N)?(\"|'))"
      ,"end": "(?!\\G)"
      ,"patterns": [
          {
              "name": "string.quoted.double"
              ,"begin": "(?i)\\G(N)?\""
              ,"end": "\"(?=[^\"])|\n"
              ,"patterns": [
                  {
                      "name": "constant.character.escape"
                      ,"match": "\"\""
                  }        
              ]
          }
          ,{
              "name": "string.quoted.single"
              ,"begin": "(?i)\\G(N)?'"
              ,"end": "'(?=[^'])|\n"
              ,"patterns": [
                  {
                      "name": "constant.character.escape"
                      ,"match": "''"
                  }
              ]
          }
      ]
    }
    ,"cobol-name": {
      "name": "variable.other"
      ,"match": "[$a-zA-Z0-9_]([-$a-zA-Z0-9_]*[$a-zA-Z0-9_])?"
    }
    ,"comment-is-name": {
      "begin": "^[*/].*$"
      ,"beginCaptures": {
        "0": {
          "name": "comment.line"
        }
      }
      ,"end": "(?<=\\S)(?!\\G)"
      ,"patterns": [
        {
          "include": "#is-name"
        }
        ,{
          "include": "#comment-is-name"
        }
        ,{
          "include": "#cobol-name"
        }
      ]
    }
    ,"is-name": {
      "begin": "(?i)(^|(?<=[^-a-z0-9$_]))(IS)(?=[^-a-z0-9$_])"
      ,"beginCaptures": {
        "0": {
          "name": "entity.name.function"
        }
      }
      ,"end": "(?<=\\S)(?!\\G)"
      ,"patterns": [
        {
          "include": "#comment-name"
        }
        ,{
          "include": "#cobol-name"
        }
      ]
    }
    ,"comment-name": {
      "begin": "^[*/].*$"
      ,"beginCaptures": {
        "0": {
          "name": "comment.line"
        }
      }
      ,"end": "(?<=\\S)(?!\\G)"
      ,"patterns": [
        {
          "include": "#comment-name"
        }
        ,{
          "include": "#cobol-name"
        }
      ]
    }


    ,"identification-division": {
      "name": "cobol.identification.division"
      ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(IDENTIFICATION)(?=[^-a-z0-9$_])"
      ,"beginCaptures": {
        "0": {
          "name": "keyword"
        }
      }
      ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(ENVIRONMENT|DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
      ,"patterns": [
        {
          "begin": "(?i)(^|(?<=[^-a-z0-9$_]))(DIVISION)(?=[^-a-z0-9$_])"
          ,"beginCaptures": {
            "0": {
              "name": "keyword"
            }
          }
          ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(ENVIRONMENT|DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
          ,"patterns": [
            {
              "include": "#comment"
            }
            ,{
              "include": "#program-id"
            }
            ,{
              "include": "#author"
            }
            ,{
              "include": "#installation"
            }
            ,{
              "include": "#date-written"
            }
            ,{
              "include": "#date-compiled"
            }
            ,{
              "include": "#security"
            }
            ,{
              "include": "#options"
            }
          ]
        }
        ,{
          "include": "invalid-char"
        }
      ]
    }

    ,"program-id": {
      "name": "cobol.program.id"
      ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(PROGRAM-ID)(?=[^-a-z0-9$_])"
      ,"beginCaptures": {
        "0": {
          "name": "keyword"
        }
      }
      ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(AUTHOR|INSTALLATION|DATE-WRITTEN|DATE-COMPILED|SECURITY|OPTIONS|ENVIRONMENT|DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
      ,"patterns": [
        {
          "include": "#comment"
        }
        ,{
          "name": "entity.name.function"
          ,"match": "(?i)(^|(?<=[^-a-z0-9$_]))(IS|WITH|PROGRAM)(?=[^-a-z0-9$_])"
        }
        ,{
          "name": "entity.name.class"
          ,"match": "(?i)(^|(?<=[^-a-z0-9$_]))(COMMON|INITIAL)(?=[^-a-z0-9$_])"
        }
        ,{
          "begin": "(?i)(^|(?<=[^-a-z0-9$_]))(IDENT)(?=[^-a-z0-9$_])"
          ,"end": "(?=\\.)"
          ,"beginCaptures": {
            "0": {
              "name": "entity.name.class"
            }
          }
          ,"patterns": [
            {
              "include": "#string"
            }
          ]
        }
        ,{
          "include": "#cobol-name"
        }
      ]
    }

    ,"author": {
      "name": "cobol.author"
      ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(AUTHOR)(?=[^-a-z0-9$_])"
      ,"beginCaptures": {
        "0": {
          "name": "keyword"
        }
      }
      ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(INSTALLATION|DATE-WRITTEN|DATE-COMPILED|SECURITY|OPTIONS|ENVIRONMENT|DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
      ,"patterns": [
        {
          "include": "#comment"
        }
      ]
    }

    ,"installation": {
      "name": "cobol.installation"
      ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(INSTALLATION)(?=[^-a-z0-9$_])"
      ,"beginCaptures": {
        "0": {
          "name": "keyword"
        }
      }
      ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(AUTHOR|DATE-WRITTEN|DATE-COMPILED|SECURITY|OPTIONS|ENVIRONMENT|DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
      ,"patterns": [
        {
          "include": "#comment"
        }
      ]
    }

    ,"date-written": {
      "name": "cobol.date.written"
      ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(DATE-WRITTEN)(?=[^-a-z0-9$_])"
      ,"beginCaptures": {
        "0": {
          "name": "keyword"
        }
      }
      ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(INSTALLATION|AUTHOR|DATE-COMPILED|SECURITY|OPTIONS|ENVIRONMENT|DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
      ,"patterns": [
        {
          "include": "#comment"
        }
      ]
    }

    ,"security": {
      "name": "cobol.security"
      ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(SECURITY)(?=[^-a-z0-9$_])"
      ,"beginCaptures": {
        "0": {
          "name": "keyword"
        }
      }
      ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(INSTALLATION|DATE-WRITTEN|DATE-COMPILED|AUTHOR|OPTIONS|ENVIRONMENT|DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
      ,"patterns": [
        {
          "include": "#comment"
        }
      ]
    }

    ,"date-compiled": {
      "name": "cobol.date.compiled"
      ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(DATE-COMPILED)(?=[^-a-z0-9$_])"
      ,"beginCaptures": {
        "0": {
          "name": "keyword"
        }
      }
      ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(INSTALLATION|DATE-WRITTEN|AUTHOR|SECURITY|OPTIONS|ENVIRONMENT|DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
      ,"patterns": [
        {
          "include": "#comment"
        }
      ]
    }

    ,"options": {
      "name": "cobol.options"
      ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(OPTIONS)(?=[^-a-z0-9$_])"
      ,"beginCaptures": {
        "0": {
          "name": "keyword"
        }
      }
      ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(INSTALLATION|DATE-WRITTEN|DATE-COMPILED|SECURITY|AUTHOR|ENVIRONMENT|DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
      ,"patterns": [
        {
          "include": "#comment"
        }
        ,{
          "name": "entity.name.class"
          ,"match": "(?i)(^|(?<=[^-a-z0-9$_]))(ARITHMETIC|NATIVE|STANDARD)(?=[^-a-z0-9$_])"
        }
        ,{
          "name": "entity.name.function"
          ,"match": "(?i)(^|(?<=[^-a-z0-9$_]))(IS)(?=[^-a-z0-9$_])"
        }
      ]
    }

    ,"environment-division": {
      "name": "cobol.environment.division"
      ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(ENVIRONMENT)(?=[^-a-z0-9$_])"
      ,"beginCaptures": {
        "0": {
          "name": "keyword"
        }
      }
      ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
      ,"patterns": [
        {
          "begin": "(?i)(^|(?<=[^-a-z0-9$_]))(DIVISION)(?=[^-a-z0-9$_])"
          ,"beginCaptures": {
            "0": {
              "name": "keyword"
            }
          }
          ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
          ,"patterns": [
            {
              "include": "#comment"
            }
            ,{
              "include": "#configuration-section"
            }
            ,{
              "include": "#input-output-section"
            }
          ]
        }
        ,{
          "include": "invalid-char"
        }
      ]
    }

    ,"configuration-section": {
      "name": "cobol.configuration.section"
      ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(CONFIGURATION)(?=[^-a-z0-9$_])"
      ,"beginCaptures": {
        "0": {
          "name": "keyword"
        }
      }
      ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(INPUT-OUTPUT|DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
      ,"patterns": [
        {
          "begin": "(?i)(^|(?<=[^-a-z0-9$_]))(SECTION)(?=[^-a-z0-9$_])"
          ,"beginCaptures": {
            "0": {
              "name": "keyword"
            }
          }
          ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(INPUT-OUTPUT|DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
          ,"patterns": [
            {
              "include": "#comment"
            }
            ,{
              "include": "#source-computer"
            }
            ,{
              "include": "#object-computer"
            }
            ,{
              "include": "#special-names"
            }
          ]
        }
        ,{
          "include": "invalid-char"
        }
      ]
    }

    ,"source-computer": {
      "name": "cobol.source.computer"
      ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(SOURCE-COMPUTER)(?=[^-a-z0-9$_])"
      ,"beginCaptures": {
        "0": {
          "name": "keyword"
        }
      }
      ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(OBJECT-COMPUTER|SPECIAL-NAMES|INPUT-OUTPUT|DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
      ,"patterns": [
        {
          "include": "#comment"
        }
        ,{
          "name": "variable.other"
          ,"match": "(?i)(^|(?<=[^-a-z0-9$_]))(ALPHA|I64|VAX)(?=[^-a-z0-9$_])"
        }
        ,{
          "name": "entity.name.function"
          ,"match": "(?i)(^|(?<=[^-a-z0-9$_]))(WITH)(?=[^-a-z0-9$_])"
        }
        ,{
          "name": "entity.name.class"
          ,"match": "(?i)(^|(?<=[^-a-z0-9$_]))(DEBUGGING|MODE)(?=[^-a-z0-9$_])"
        }
        ,{
          "include": "#cobol-name"
        }
      ]
    }

    ,"object-computer": {
      "name": "cobol.object.computer"
      ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(OBJECT-COMPUTER)(?=[^-a-z0-9$_])"
      ,"beginCaptures": {
        "0": {
          "name": "keyword"
        }
      }
      ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(SOURCE-COMPUTER|SPECIAL-NAMES|INPUT-OUTPUT|DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
      ,"patterns": [
        {
          "include": "#comment"
        }
        ,{
          "name": "variable.other"
          ,"match": "(?i)(^|(?<=[^-a-z0-9$_]))(ALPHA|I64|VAX)(?=[^-a-z0-9$_])"
        }
        ,{
          "name": "cobol.memory"
          ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(MEMORY)(?=[^-a-z0-9$_])"
          ,"beginCaptures": {
            "0": {
              "name": "entity.name.class"
            }
          }
          ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(SEGMENT-LIMIT|PROGRAM|COLLATING|SEQUENCE|SOURCE-COMPUTER|SPECIAL-NAMES|INPUT-OUTPUT|DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
          ,"patterns": [
            {
              "name": "entity.name.function"
              ,"match": "(?i)(^|(?<=[^-a-z0-9$_]))(SIZE)(?=[^-a-z0-9$_])"
            }
            ,{
              "name": "constant.numeric"
              ,"match": "(?i)(^|(?<=[^-a-z0-9$_]))(\\d+)(?=[^-a-z0-9$_])"
            }
            ,{
              "name": "entity.name.class"
              ,"match": "(?i)(^|(?<=[^-a-z0-9$_]))(WORDS|CHARACTERS|MODULES)(?=[^-a-z0-9$_])"
            }
          ]
        }
        ,{
          "name": "cobol.sequence"
          ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(PROGRAM|COLLATING|(SEQUENCE))(?=[^-a-z0-9$_])"
          ,"beginCaptures": {
            "2": {
              "name": "entity.name.function"
            }
            ,"3": {
              "name": "entity.name.class"
            }
          }
          ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(SEGMENT-LIMIT|SOURCE-COMPUTER|SPECIAL-NAMES|INPUT-OUTPUT|DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
          ,"patterns": [
            {
              "match": "(?i)(^|(?<=[^-a-z0-9$_]))(PROGRAM|COLLATING|(SEQUENCE)|IS)(?=[^-a-z0-9$_])"
              ,"captures": {
                "3": {
                  "name": "entity.name.class"
                }
              }
              ,"name": "entity.name.function"
            }
            ,{
              "include": "#cobol-name"
            }
          ]
        }
        ,{
          "name": "cobol.segment.limit"
          ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(SEGMENT-LIMIT)(?=[^-a-z0-9$_])"
          ,"beginCaptures": {
            "2": {
              "name": "entity.name.class"
            }
          }
          ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(SOURCE-COMPUTER|SPECIAL-NAMES|INPUT-OUTPUT|DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
          ,"patterns": [
            {
              "name": "entity.name.function"
              ,"match": "(?i)(^|(?<=[^-a-z0-9$_]))(IS)(?=[^-a-z0-9$_])"
            }
            ,{
              "name": "constant.numeric"
              ,"match": "(?i)(^|(?<=[^-a-z0-9$_]))(\\d+)(?=[^-a-z0-9$_])"
            }
          ]
        }
        ,{
          "include": "#cobol-name"
        }
      ]
    }

    ,"special-names": {
      "name": "cobol.special.names"
      ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(SPECIAL-NAMES)(?=[^-a-z0-9$_])"
      ,"beginCaptures": {
        "0": {
          "name": "keyword"
        }
      }
      ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(SOURCE-COMPUTER|OBJECT-COMPUTER|INPUT-OUTPUT|DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
      ,"patterns": [
        {
          "include": "#comment"
        }
        ,{
          "include": "#device"
        }
        ,{
          "include": "#argument-environment"
        }
        ,{
          "include": "#c01"
        }
        ,{
          "include": "#switch"
        }
        ,{
          "include": "#alphabet"
        }
        ,{
          "include": "#symbolic"
        }
        ,{
          "include": "#class"
        }
        ,{
          "include": "#currency"
        }
        ,{
          "include": "#decimal-point"
        }
        ,{
          "include": "#cursor"
        }
        ,{
          "include": "#crt"
        }
      ]
    }
    ,"device": {
      "name": "cobol.device"
      ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(CARD-READER|PAPER-TAPE-READER|CONSOLE|LINE-PRINTER|PAPER-TAPE-PUNCH|SYSIN|SYSOUT|SYSERR)(?=[^-a-z0-9$_])"
      ,"beginCaptures": {
        "2": {
          "name": "entity.name.class"
        }
      }
      ,"end": "(?<=\\S)(?!\\G)"
      ,"patterns": [
        {
          "include": "#is-name"
        }
        ,{
          "include": "#comment-is-name"
        }
        ,{
          "include": "#cobol-name"
        }
      ]
    }
    ,"argument-environment": {
      "name": "cobol.argument_environment"
      ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(ARGUMENT-NUMBER|ARGUMENT-VALUE|ENVIRONMENT-NAME|ENVIRONMENT-VALUE)(?=[^-a-z0-9$_])"
      ,"beginCaptures": {
        "2": {
          "name": "entity.name.class"
        }
      }
      ,"end": "(?<=\\S)(?!\\G)"
      ,"patterns": [
        {
          "include": "#is-name"
        }
        ,{
          "include": "#comment-is-name"
        }
        ,{
          "include": "#cobol-name"
        }
      ]
    }
    ,"c01": {
      "name": "cobol.c01"
      ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(C01)(?=[^-a-z0-9$_])"
      ,"beginCaptures": {
        "2": {
          "name": "entity.name.class"
        }
      }
      ,"end": "(?<=\\S)(?!\\G)"
      ,"patterns": [
        {
          "include": "#is-name"
        }
        ,{
          "include": "#comment-is-name"
        }
        ,{
          "include": "#cobol-name"
        }
      ]
    }
    ,"switch": {
      "name": "cobol.switch"
      ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(SWITCH(-\\d(\\d)?)?)(?=[^-a-z0-9$_])"
      ,"beginCaptures": {
        "2": {
          "name": "entity.name.class"
        }
      }
      ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(SWITCH(-\\d(\\d)?)?|CARD-READER|PAPER-TAPE-READER|CONSOLE|LINE-PRINTER|PAPER-TAPE-PUNCH|SYSIN|SYSOUT|SYSERR|C01|ARGUMENT-NUMBER|ARGUMENT-VALUE|ENVIRONMENT-NAME|ENVIRONMENT-VALUE|ALPHABET|SYMBOLIC|CLASS|CURRENCY|DECIMAL-POINT|CURSOR|CRT|SOURCE-COMPUTER|OBJECT-COMPUTER|INPUT-OUTPUT|DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
      ,"patterns": [
        {
          "include": "#comment"
        }
        ,{
          "name": "entity.name.function"
          ,"match": "(?i)(^|(?<=[^-a-z0-9$_]))(STATUS|IS)(?=[^-a-z0-9$_])"
        }
        ,{
          "name": "entity.name.class"
          ,"match": "(?i)(^|(?<=[^-a-z0-9$_]))(ON|OFF)(?=[^-a-z0-9$_])"
        }
        ,{
          "name": "constant.numeric"
          ,"match": "(?i)(^|(?<=[^-a-z0-9$_]))(\\d+)(?=[^-a-z0-9$_])"
        }
        ,{
          "include": "#cobol-name"
        }
      ]
    }
    ,"alphabet": {
      "name": "cobol.alphabet"
      ,"begin": "(?i)(^|(?<=[^-a-z0-9$_]))(ALPHABET)(?=[^-a-z0-9$_])"
      ,"beginCaptures": {
        "2": {
          "name": "entity.name.class"
        }
      }
      ,"end": "(?i)(^|(?<=[^-a-z0-9$_]))(?=(SWITCH(-\\d(\\d)?)?|CARD-READER|PAPER-TAPE-READER|CONSOLE|LINE-PRINTER|PAPER-TAPE-PUNCH|SYSIN|SYSOUT|SYSERR|C01|ARGUMENT-NUMBER|ARGUMENT-VALUE|ENVIRONMENT-NAME|ENVIRONMENT-VALUE|ALPHABET|SYMBOLIC|CLASS|CURRENCY|DECIMAL-POINT|CURSOR|CRT|SOURCE-COMPUTER|OBJECT-COMPUTER|INPUT-OUTPUT|DATA|PROCEDURE|END|IDENTIFICATION)[^-a-z0-9$_])"
      ,"patterns": [
        {
          "include": "#comment"
        }
        ,{
          "name": "entity.name.function"
          ,"match": "(?i)(^|(?<=[^-a-z0-9$_]))(IS)(?=[^-a-z0-9$_])"
