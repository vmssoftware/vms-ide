            },
            {
                "command": "vmssoftware.synchronizer.downloadHeaders",
                "title": "%synchronizer.command.downloadHeaders.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.collectJavaClasses",
                "title": "%synchronizer.command.collectJavaClasses.title%",
                "category": "%command.category%"
            }
        ],
        "configuration": {
            "title": "%configuration.title%",
            "properties": {
                "vmssoftware.config-helper.settings.using": {
                    "scope": "resource",
                    "type": "string",
                    "enum": [
                        "VSC",
                        "FS",
                        "VFS"
                    ],
                    "enumDescriptions": [
                        "%config-helper.configuration.vsc.description%",
                        "%config-helper.configuration.fs.description%",
                        "%config-helper.configuration.vfs.description%"
                    ],
                    "default": "FS",
                    "description": "%config-helper.configuration.type.description%"
                },
                "vmssoftware.config-helper.settings.debug": {
                    "scope": "resource",
                    "type": "string",
                    "default": "",
                    "description": "%config-helper.configuration.debug.description%"
                },
                "vmssoftware.config-helper.settings.addCalleeInfo": {
                    "scope": "resource",
                    "type": "boolean",
                    "default": false,
                    "description": "%config-helper.configuration.addCalleeInfo.description%"
                },
                "vmssoftware.ssh-helper.connection.host": {
                    "scope": "resource",
                    "type": "string",
                    "default": "host",
                    "description": "%ssh-helper.configuration.host%"
                },
                "vmssoftware.ssh-helper.connection.port": {
                    "scope": "resource",
                    "type": "number",
                    "default": 22,
                    "description": "%ssh-helper.configuration.port%"
                },
                "vmssoftware.ssh-helper.connection.username": {
                    "scope": "resource",
                    "type": "string",
                    "default": "",
                    "description": "%ssh-helper.configuration.username%"
                },
                "vmssoftware.ssh-helper.connection.password": {
                    "scope": "resource",
                    "type": "string",
                    "default": "",
                    "description": "%ssh-helper.configuration.password%"
                },
                "vmssoftware.ssh-helper.connection.keyFile": {
                    "scope": "resource",
                    "type": "string",
                    "default": "",
                    "description": "%ssh-helper.configuration.keyFile%"
                },
                "vmssoftware.ssh-helper.timeouts.cmdTimeout": {
                    "scope": "resource",
                    "type": "number",
                    "default": 0,
                    "description": "%ssh-helper.configuration.timeouts.command%"
                },
                "vmssoftware.ssh-helper.timeouts.feedbackTimeout": {
                    "scope": "resource",
                    "type": "number",
                    "default": 0,
                    "description": "%ssh-helper.configuration.timeouts.feedback%"
                },
                "vmssoftware.ssh-helper.timeouts.welcomeTimeout": {
                    "scope": "resource",
                    "type": "number",
                    "default": 0,
                    "description": "%ssh-helper.configuration.timeouts.welcome%"
                },
                "vmssoftware.ssh-helper.terminal.command": {
                    "scope": "resource",
                    "type": "string",
                    "default": "",
                    "description": "%ssh-helper.configuration.terminal.command%"
                },
                "vmssoftware.ssh-helper.host-collection.hosts": {
                    "scope": "resource",
                    "type": "array",
                    "default": [
                        {
                            "label": "",
                            "host": "",
                            "port": 22,
                            "username": "",
                            "password": "",
                            "keyFile": ""
                        }
                    ],
                    "description": "%ssh-helper.configuration.hosts-collection%"
                },
                "vmssoftware.project-dep.buildName": {
                    "scope": "resource",
                    "type": "string"
                },
                "vmssoftware.project-dep.dependencies": {
                    "scope": "resource",
                    "type": "array",
                    "default": [
                        {
                            "folder": "",
                            "depends": [
                                ""
                            ]
                        }
                    ],
                    "description": "Project dependencies"
                },
                "vmssoftware.synchronizer.project.break": {
                    "scope": "resource",
                    "type": "string",
                    "default": "C",
                    "description": "%synchronizer.properties.project.break.description%"
                },
                "vmssoftware.synchronizer.project.builders": {
                    "scope": "resource",
                    "type": "string",
                    "default": "*.{mms,com,opt}",
                    "description": "%synchronizer.properties.project.builders.description%"
                },
                "vmssoftware.synchronizer.project.exclude": {
                    "scope": "resource",
                    "type": "string",
                    "default": "**/{.vscode,node-modules,out}",
                    "description": "%synchronizer.properties.project.exclude.description%"
                },
                "vmssoftware.synchronizer.project.headers": {
                    "scope": "resource",
                    "type": "string",
                    "default": "*.{h,hpp}",
                    "description": "%synchronizer.properties.project.headers.description%"
                },
                "vmssoftware.synchronizer.project.listing": {
                    "scope": "resource",
                    "type": "string",
                    "default": "*.lis",
                    "description": "%synchronizer.properties.project.listing.description%"
                },
                "vmssoftware.synchronizer.project.outdir": {
                    "scope": "resource",
                    "type": "string",
                    "default": "out",
                    "description": "%synchronizer.properties.project.outdir.description%"
                },
                "vmssoftware.synchronizer.project.projectName": {
                    "scope": "resource",
                    "type": "string",
                    "default": "project",
                    "description": "%synchronizer.properties.project.projectName.description%"
                },
                "vmssoftware.synchronizer.project.projectType": {
                    "scope": "resource",
                    "type": "string",
                    "enum": [
                        "executable",
                        "library",
                        "shareable",
                        "java",
                        "kotlin",
                        "scala"
                    ],
                    "enumDescriptions": [
                        "Executable",
                        "Static library",
                        "Shareable image",
                        "Java project",
                        "Kotlin project",
                        "Scala project"
                    ],
                    "default": "executable",
                    "description": "%synchronizer.properties.project.projectType.description%"
                },
                "vmssoftware.synchronizer.project.resource": {
                    "scope": "resource",
                    "type": "string",
                    "default": "**/resource/**/*.*",
                    "description": "%synchronizer.properties.project.resource.description%"
                },
                "vmssoftware.synchronizer.project.root": {
                    "scope": "resource",
                    "type": "string",
                    "default": "project",
                    "description": "%synchronizer.properties.project.root.description%"
                },
                "vmssoftware.synchronizer.project.source": {
                    "scope": "resource",
                    "type": "string",
                    "default": "*.{c,cpp}",
                    "description": "%synchronizer.properties.project.source.description%"
                },
                "vmssoftware.synchronizer.project.addLibraries": {
                    "scope": "resource",
                    "type": "string",
                    "default": "",
                    "description": "%synchronizer.properties.project.addLibraries.description%"
                },
                "vmssoftware.synchronizer.project.addIncludes": {
                    "scope": "resource",
                    "type": "string",
                    "default": "",
                    "description": "%synchronizer.properties.project.addIncludes.description%"
                },
                "vmssoftware.synchronizer.synchronize.downloadNewFiles": {
                    "scope": "resource",
                    "type": "string",
                    "enum": [
                        "overwrite",
                        "skip",
                        "edit"
                    ],
                    "enumDescriptions": [
                        "%synchronizer.properties.synchronize.downloadNewFiles.overwrite.description%",
                        "%synchronizer.properties.synchronize.downloadNewFiles.skip.description%",
                        "%synchronizer.properties.synchronize.downloadNewFiles.edit.description%"
                    ],
                    "default": "edit",
                    "description": "%synchronizer.properties.synchronize.downloadNewFiles.description%"
                },
                "vmssoftware.synchronizer.synchronize.keepAlive": {
                    "scope": "resource",
                    "type": "boolean",
                    "default": false,
                    "description": "%synchronizer.properties.synchronize.keepAlive.description%"
                },
                "vmssoftware.synchronizer.synchronize.setTimeAttempts": {
                    "scope": "resource",
                    "type": "number",
                    "default": 3,
                    "description": "%synchronizer.properties.synchronize.setTimeAttempts.description%"
                },
                "vmssoftware.synchronizer.synchronize.setTimeByShell": {
                    "scope": "resource",
                    "type": "boolean",
                    "default": true,
                    "description": "%synchronizer.properties.synchronize.setTimeByShell.description%"
                },
                "vmssoftware.synchronizer.synchronize.unzipCmd": {
                    "scope": "resource",
                    "type": "string",
                    "default": "",
                    "description": "%synchronizer.properties.synchronize.unzipCmd.description%"
                },
                "vmssoftware.synchronizer.synchronize.preferZip": {
                    "scope": "resource",
                    "type": "boolean",
                    "default": false,
                    "description": "%synchronizer.properties.synchronize.preferZip.description%"
                },
                "vmssoftware.synchronizer.synchronize.forceLocalTime": {
                    "scope": "resource",
                    "type": "boolean",
                    "default": true,
                    "description": "%synchronizer.properties.synchronize.forceLocalTime.description%"
                },
                "vmssoftware.synchronizer.synchronize.purge": {
                    "scope": "resource",
                    "type": "boolean",
                    "default": false,
                    "description": "%synchronizer.properties.synchronize.purge.description%"
                },
                "vmssoftware.synchronizer.builds.configurations": {
                    "scope": "resource",
                    "type": "array",
                    "default": [
                        {
                            "label": "DEBUG",
                            "description": "debug",
                            "command": "",
                            "parameter": "DEBUG"
                        },
                        {
                            "label": "RELEASE",
                            "description": "release",
                            "command": "",
                            "parameter": "RELEASE"
                        }
                    ],
                    "description": "%synchronizer.properties.builds.configurations.description%"
                }
            }
        },
        "breakpoints": [
            {
                "language": "c"
            },
            {
                "language": "cpp"
            },
            {
                "language": "bliss"
            },
            {
                "language": "basic"
            },
            {
                "language": "COBOL"
            },
            {
                "language": "fortran"
