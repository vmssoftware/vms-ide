            },
            {
                "command": "vmssoftware.synchronizer.buildProject",
                "title": "%synchronizer.command.buildProject.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.reBuildProject",
                "title": "%synchronizer.command.reBuildProject.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.buildOnlyProject",
                "title": "%synchronizer.command.buildOnlyProject.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.reBuildOnlyProject",
                "title": "%synchronizer.command.reBuildOnlyProject.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.cleanProject",
                "title": "%synchronizer.command.cleanProject.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.syncProject",
                "title": "%synchronizer.command.syncProject.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.editProject",
                "title": "%synchronizer.command.editProject.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.changeCRLF",
                "title": "%synchronizer.command.changeCRLF.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.forceSynchronized",
                "title": "%synchronizer.command.forceSynchronized.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.forceBuilt",
                "title": "%synchronizer.command.forceBuilt.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.uploadZip",
                "title": "%synchronizer.command.uploadZip.title%",
