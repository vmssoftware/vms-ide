{
    "name": "vms-ide",
    "displayName": "VMS IDE",
    "description": "VMS IDE is an extension that enables you to develop applications for OpenVMS.",
    "version": "0.0.22",
    "publisher": "VMSSoftwareInc",
    "repository": {
        "type": "git",
        "url": "git:"
    },
    "engines": {
        "vscode": "^1.33.1"
    },
    "categories": [
        "Debuggers",
        "Programming Languages",
        "Other"
    ],
    "icon": "images/vms_128x128_v2.png",
    "main": "./out/extension",
    "activationEvents": [
        "*",
        "onView:vmssoftware.project-dep.projectDependencies",
        "onView:vmssoftware.project-dep.projectDescription",
        "onCommand:vmssoftware.ssh-helper.editSettings",
        "onCommand:vmssoftware.synchronizer.stopAction",
        "onCommand:vmssoftware.synchronizer.buildProject",
        "onCommand:vmssoftware.synchronizer.reBuildProject",
        "onCommand:vmssoftware.synchronizer.buildOnlyProject",
        "onCommand:vmssoftware.synchronizer.reBuildOnlyProject",
        "onCommand:vmssoftware.synchronizer.createMMS",
        "onCommand:vmssoftware.synchronizer.cleanProject",
        "onCommand:vmssoftware.synchronizer.syncProject",
        "onCommand:vmssoftware.synchronizer.editProject",
        "onCommand:vmssoftware.synchronizer.changeCRLF",
        "onCommand:vmssoftware.synchronizer.forceSynchronized",
        "onCommand:vmssoftware.synchronizer.forceBuilt",
        "onCommand:vmssoftware.synchronizer.uploadZip",
        "onCommand:vmssoftware.synchronizer.upload",
        "onCommand:vmssoftware.synchronizer.downloadHeaders",
        "onCommand:vmssoftware.synchronizer.collectJavaClasses",
        "onDebug",
        "onCommand:extension.vms-debug.terminal",
        "onLanguage:cld",
        "onLanguage:msg",
        "onLanguage:mms",
        "onCommand:workbench.action.tasks.runTask"
    ],
    "contributes": {
        "taskDefinitions": [
            {
                "type": "task2cmd",
                "required": [
                    "command"
                ],
                "properties": {
                    "command": {
                        "type": "string",
                        "description": "Command to execute"
                    },
                    "label": {
                        "type": "string",
                        "description": "Label to display"
                    },
                    "extension": {
                        "type": "string",
                        "description": "Extension which executes command"
                    },
                    "parameter": {
                        "type": "string",
                        "description": "One string parameter"
                    }
                }
            }
        ],
        "languages": [
            {
                "id": "mms",
                "aliases": [
                    "MMS",
                    "mms"
                ],
                "extensions": [
                    ".mms"
                ],
                "configuration": "mms.language-configuration.json"
            },
            {
                "id": "cld",
                "aliases": [
                    "CLD",
                    "cld"
                ],
                "extensions": [
                    ".cld"
                ],
                "configuration": "cld.language-configuration.json"
            },
            {
                "id": "msg",
                "aliases": [
                    "MSG",
                    "msg"
                ],
                "extensions": [
                    ".msg"
                ],
                "configuration": "msg.language-configuration.json"
            },
            {
                "id": "bliss",
                "extensions": [
                    ".bli"
                ]
            },
            {
                "id": "basic",
                "extensions": [
                    ".bas"
                ]
            },
            {
                "id": "COBOL",
                "extensions": [
                    ".cob"
                ]
            },
            {
                "id": "fortran",
                "extensions": [
                    ".f77",
                    ".f90"
                ]
            },
            {
                "id": "pascal",
                "extensions": [
                    ".pas"
                ]
            },
            {
                "id": "c",
                "extensions": [
                    ".c"
                ]
            },
            {
                "id": "cpp",
                "extensions": [
                    ".cpp"
                ]
            }
        ],
        "grammars": [
            {
                "language": "mms",
                "scopeName": "source.mms",
                "path": "syntaxes/mms.tmLanguage.json"
            },
            {
                "language": "cld",
                "scopeName": "source.cld",
                "path": "syntaxes/cld.tmLanguage.json"
            },
            {
                "language": "msg",
                "scopeName": "source.msg",
                "path": "syntaxes/msg.tmLanguage.json"
            }
        ],
        "keybindings": [
            {
                "command": "extension.vms-debug.terminal",
                "key": "Ctrl+F6"
            },
            {
                "command": "vmssoftware.synchronizer.buildProject",
                "key": "Ctrl+F7"
            },
            {
                "command": "vmssoftware.synchronizer.buildOnlyProject",
                "key": "Ctrl+Alt+F7"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "project-explorer",
                    "title": "Project Explorer",
                    "icon": "media/dep.svg"
                }
            ]
        },
        "views": {
            "project-explorer": [
                {
                    "id": "vmssoftware.project-dep.projectDependencies",
                    "name": "Project Dependencies"
                },
                {
                    "id": "vmssoftware.project-dep.projectDescription",
                    "name": "Project description"
                }
            ]
        },
        "menus": {
            "commandPalette": [
                {
                    "command": "vmssoftware.ssh-helper.editSettings"
                },
                {
                    "command": "vmssoftware.synchronizer.stopAction",
                    "when": "vmssoftware.synchronizer.isInAction"
                },
                {
                    "command": "vmssoftware.synchronizer.buildProject",
                    "when": "!vmssoftware.synchronizer.isInAction"
                },
                {
                    "command": "vmssoftware.synchronizer.reBuildProject",
                    "when": "!vmssoftware.synchronizer.isInAction"
                },
                {
                    "command": "vmssoftware.synchronizer.buildOnlyProject",
                    "when": "!vmssoftware.synchronizer.isInAction"
                },
                {
                    "command": "vmssoftware.synchronizer.reBuildOnlyProject",
                    "when": "!vmssoftware.synchronizer.isInAction"
                },
                {
                    "command": "vmssoftware.synchronizer.cleanProject",
                    "when": "!vmssoftware.synchronizer.isInAction"
                },
                {
                    "command": "vmssoftware.synchronizer.syncProject",
                    "when": "!vmssoftware.synchronizer.isInAction"
                },
                {
                    "command": "vmssoftware.synchronizer.editProject",
                    "when": "!vmssoftware.synchronizer.isInAction"
                },
                {
                    "command": "vmssoftware.synchronizer.changeCRLF",
                    "when": "!vmssoftware.synchronizer.isInAction"
                },
                {
                    "command": "vmssoftware.synchronizer.uploadZip",
                    "when": "!vmssoftware.synchronizer.isInAction"
                },
                {
                    "command": "vmssoftware.synchronizer.upload",
                    "when": "!vmssoftware.synchronizer.isInAction"
                },
                {
                    "command": "vmssoftware.synchronizer.cleanProject",
                    "when": "!vmssoftware.synchronizer.isInAction"
                },
                {
                    "command": "vmssoftware.synchronizer.downloadHeaders",
                    "when": "!vmssoftware.synchronizer.isInAction"
                },
                {
                    "command": "vmssoftware.synchronizer.collectJavaClasses",
                    "when": "!vmssoftware.synchronizer.isInAction"
                }
            ],
            "view/title": [
                {
                    "command": "vmssoftware.project-dep.projectDependencies.refresh",
                    "when": "view == vmssoftware.project-dep.projectDependencies",
                    "group": "navigation"
                },
                {
                    "command": "vmssoftware.project-dep.projectDescription.refresh",
                    "when": "view == vmssoftware.project-dep.projectDescription",
                    "group": "navigation"
                }
            ],
            "view/item/context": [
                {
                    "command": "vmssoftware.project-dep.projectDependencies.add",
                    "when": "view == vmssoftware.project-dep.projectDependencies && viewItem == folder"
                },
                {
                    "command": "vmssoftware.project-dep.projectDependencies.syncProject",
                    "when": "view == vmssoftware.project-dep.projectDependencies && viewItem == folder"
                },
                {
                    "command": "vmssoftware.project-dep.projectDependencies.upload",
                    "when": "view == vmssoftware.project-dep.projectDependencies && viewItem == folder"
                },
                {
                    "command": "vmssoftware.project-dep.projectDependencies.build",
                    "when": "view == vmssoftware.project-dep.projectDependencies && viewItem == folder"
                },
                {
                    "command": "vmssoftware.project-dep.projectDependencies.rebuild",
                    "when": "view == vmssoftware.project-dep.projectDependencies && viewItem == folder"
                },
                {
                    "command": "vmssoftware.project-dep.projectDependencies.buildOnly",
                    "when": "view == vmssoftware.project-dep.projectDependencies && viewItem == folder"
                },
                {
                    "command": "vmssoftware.project-dep.projectDependencies.rebuildOnly",
                    "when": "view == vmssoftware.project-dep.projectDependencies && viewItem == folder"
                },
                {
                    "command": "vmssoftware.project-dep.projectDependencies.clean",
                    "when": "view == vmssoftware.project-dep.projectDependencies && viewItem == folder"
                },
                {
                    "command": "vmssoftware.project-dep.projectDependencies.remove",
                    "when": "view == vmssoftware.project-dep.projectDependencies && viewItem == depends"
                },
                {
                    "command": "vmssoftware.project-dep.projectDescription.edit",
                    "when": "view == vmssoftware.project-dep.projectDescription && viewItem == editable"
                },
                {
                    "command": "vmssoftware.project-dep.projectDescription.changeBuildName",
                    "when": "view == vmssoftware.project-dep.projectDescription && viewItem == buildName"
                }
            ]
        },
        "commands": [
            {
                "command": "extension.vms-debug.terminal",
                "title": "%vms_debug.extension.vms_terminal%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.ssh-helper.editSettings",
                "title": "%ssh-helper.command.edit.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.project-dep.projectDependencies.refresh",
                "title": "%synchronizer.command.projectDependencies.refresh.title%",
                "icon": {
                    "light": "resources/light/refresh.svg",
                    "dark": "resources/dark/refresh.svg"
                }
            },
            {
                "command": "vmssoftware.project-dep.projectDependencies.add",
                "title": "%synchronizer.command.projectDependencies.add.title%"
            },
            {
                "command": "vmssoftware.project-dep.projectDependencies.syncProject",
                "title": "%synchronizer.command.projectDependencies.syncProject.title%"
            },
            {
                "command": "vmssoftware.project-dep.projectDependencies.upload",
                "title": "%synchronizer.command.projectDependencies.upload.title%"
            },
            {
                "command": "vmssoftware.project-dep.projectDependencies.build",
                "title": "%synchronizer.command.projectDependencies.build.title%"
            },
            {
                "command": "vmssoftware.project-dep.projectDependencies.rebuild",
                "title": "%synchronizer.command.projectDependencies.rebuild.title%"
            },
            {
                "command": "vmssoftware.project-dep.projectDependencies.buildOnly",
                "title": "%synchronizer.command.projectDependencies.buildOnly.title%"
            },
            {
                "command": "vmssoftware.project-dep.projectDependencies.rebuildOnly",
                "title": "%synchronizer.command.projectDependencies.rebuildOnly.title%"
            },
            {
                "command": "vmssoftware.project-dep.projectDependencies.clean",
                "title": "%synchronizer.command.projectDependencies.clean.title%"
            },
            {
                "command": "vmssoftware.project-dep.projectDependencies.remove",
                "title": "%synchronizer.command.projectDependencies.remove.title%"
            },
            {
                "command": "vmssoftware.project-dep.projectDescription.refresh",
                "title": "%synchronizer.command.projectDescription.refresh.title%",
                "icon": {
                    "light": "resources/light/refresh.svg",
                    "dark": "resources/dark/refresh.svg"
                }
            },
            {
                "command": "vmssoftware.project-dep.projectDescription.select",
                "title": "%synchronizer.command.projectDescription.select.title%"
            },
            {
                "command": "vmssoftware.project-dep.projectDescription.edit",
                "title": "%synchronizer.command.projectDescription.edit.title%"
            },
            {
                "command": "vmssoftware.project-dep.projectDescription.changeBuildName",
                "title": "%synchronizer.command.projectDescription.changeBuildName.title%"
            },
            {
                "command": "vmssoftware.synchronizer.stopAction",
                "title": "%synchronizer.command.stopAction.title%",
                "category": "%command.category%"
            },
            {
                "command": "vmssoftware.synchronizer.createMMS",
                "title": "%synchronizer.command.createMMS.title%",
