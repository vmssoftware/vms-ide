{
	"name": "vms-debug",
	"displayName": "VMS Debugger",
	"version": "0.28.7",
	"publisher": "vmssoftware",
	"description": "The extension for developing debug adapters for VS Code.",
	"author": {
		"name": "VMS Software",
		"email": "akulikovskiy@vmssoftware.com"
	},
	"license": "MIT",
	"keywords": [
		"does not multi-root ready"
	],
	"engines": {
		"vscode": "^1.28.0",
		"node": "^8.x.x"
	},
	"icon": "images/mock-debug-icon.png",
	"categories": [
		"Debuggers",
		"Other"
	],
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://host/project.git"
	},
	"bugs": {
		"url": "https://host/project/issues"
	},
	"scripts": {
		"get-ext-api": "PowerShell copy -Path ..\\synchronizer\\out\\sync\\source.d.ts, ..\\synchronizer\\out\\sync\\sync-api.d.ts, ..\\ssh-helper\\out\\api.d.ts, ..\\ssh-helper\\out\\ssh-helper.d.ts, ..\\config-helper\\out\\config\\config.d.ts -Destination src\\ext-api",
		"prepublish": "tsc -p ./",
		"compile": "tsc -p ./",
		"tslint": "tslint ./**/*.ts",
		"watch": "tsc -w -p ./",
		"test": "mocha -u tdd ./out/tests/",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"package": "vsce package",
		"publish": "vsce publish"
	},
	"devDependencies": {
		"@types/mocha": "^5.x.x",
		"@types/node": "^8.10.38",
		"del": "^3.0.0",
		"event-stream": "^4.0.1",
		"fs-extra": "^7.0.1",
		"gulp": "^3.9.1",
		"gulp-sourcemaps": "^2.6.4",
		"gulp-typescript": "^5.0.0-alpha.3",
		"run-sequence": "^2.2.1",
		"tslint": "^5.11.0",
		"typescript": "^3.1.6",
		"vsce": "^1.53.0",
		"vscode": "^1.1.21",
		"vscode-nls-dev": "github:vorfol/vscode-nls-dev"
	},
	"dependencies": {
		"@vorfol/common": "^0.0.20",
		"await-notify": "^1.0.1",
		"net": "^1.0.2",
		"vscode-debugadapter": "^1.32.1",
		"vscode-debugadapter-testsupport": "1.27.0",
		"vscode-nls": "^4.0.0"
	},
	"main": "./out/extension",
	"activationEvents": [
		"onDebug",
		"onCommand:extension.vms-debug.connect",
		"onCommand:extension.vms-debug.terminal"
	],
	"contributes": {
		"keybindings": [
			{
				"command": "extension.vms-debug.terminal",
				"key": "Ctrl+F6"
			}
		],
		"commands": [
			{
				"command": "extension.vms-debug.connect",
				"title": "%extension.vms_connect%",
				"category": "%command.category%"
			},
			{
				"command": "extension.vms-debug.terminal",
				"title": "%extension.vms_terminal%",
				"category": "%command.category%"
			}
		],
		"breakpoints": [
			{
				"language": "c"
			},
			{
				"language": "cpp"
			}
		],
		"debuggers": [
			{
				"type": "vms_dbg",
				"label": "%extension.vms_c_debug%",
				"program": "./out/debugAdapter.js",
				"runtime": "node",
				"configurationAttributes": {
					"launch": {
						"required": [
							"program"
						],
						"properties": {
							"program": {
								"type": "string",
								"description": "Path from home directory to EXE file.",
								"default": ""
							},
							"stopOnEntry": {
								"type": "boolean",
								"description": "Automatically stop after launch.",
								"default": true
							},
							"trace": {
								"type": "boolean",
								"description": "Enable logging of the Debug Adapter Protocol.",
								"default": true
							},
							"typeRun": {
								"type": "string",
								"enum": [
									"DEBUG",
									"RUN"
								],
								"enumDescriptions": [
									"Use for run Debuger",
									"Use for run Program"
								],
								"default": "DEBUG",
								"description": "Type of runing"
							}
						}
					}
				},
				"initialConfigurations": [
					{
						"type": "vms_dbg",
						"request": "launch",
						"name": "Settings Debug",
						"program": "",
						"stopOnEntry": true,
						"typeRun": "DEBUG"
					},
					{
						"type": "vms_dbg",
						"request": "launch",
						"name": "Settings Run",
						"program": "",
						"typeRun": "RUN"
					}
				],
				"configurationSnippets": [
					{
						"label": "VMS C Debug: Launch",
						"description": "A new configuration for 'debugging'",
						"body": {
							"type": "vms_dbg",
							"request": "launch",
							"name": "Settings Name",
							"program": "[.path]name",
							"stopOnEntry": true,
							"typeRun": "DEBUG"
						}
					}
				]
			}
		]
	}
}
