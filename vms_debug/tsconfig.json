{
	"compileOnSave": false,
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2018",

		"noImplicitAny": false,
		"removeComments": false,
		"noUnusedLocals": true,
		"noImplicitThis": true,
		"inlineSourceMap": false,
		"sourceMap": true,
		"outDir": "./out",
		"rootDir": "./src",
		"lib": [
			"es2018"
		],
		"preserveConstEnums": true,
		"strictNullChecks": true,
		"noUnusedParameters": false
	},
	"exclude": [
		"node_modules",
		"out"
	]
}