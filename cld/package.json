{
    "name": "cld",
    "displayName": "cld",
    "description": "CLD syntax highlighting",
    "version": "0.0.1",
    "publisher": "vmssoftware",
    "repository": {
        "type": "git",
        "url": "git:vmssoftware"
    },
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
