        ,"string": {
            "name": "string.quoted.cld"
            ,"begin": "\""
            ,"end": "\"(?=[^\"])"
            ,"patterns": [
                {
                    "name": "constant.character.escape.cld"
                    ,"match": "\"\""
                }
            ]
        }
        ,"name": 
        {
            "patterns": 
            [
                {
                    "name": "name"
                    ,"match": "(?i)[a-z$][a-z$_0-9]*"
