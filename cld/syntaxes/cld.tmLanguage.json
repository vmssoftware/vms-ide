                    "name": "constant.numeric"
                }
            }
            ,"end": "(?i)(?=define|ident|module|routine|image|disallow|nodisallows|noparameters|noqualifiers|parameter|qualifier|synonym)"
            ,"patterns": [
                {
                    "include": "#expression"
                }
                ,{
                    "include": "#comment"
                }
            ]
        }
        ,"expression":
        {
            "patterns": [
                {
                    "begin": "\\("
                    ,"end": "\\)"
                    ,"patterns": [
                        {
                            "include": "#expression"
                        }
                        ,{
                            "include": "#comment"
                        }
                    ]
                }
                ,{
                    "begin": "(?i)any2"
                    ,"beginCaptures": {
                        "0":
                        {
                            "name": "constant.regexp"
                        }
                    }
                    ,"end": "\\)"
                    ,"patterns": [
                        {
                            "include": "#entity"
                        }
                        ,{
                            "include": "#comment"
                        }
                    ]
                }
                ,{
                    "match": "(?i)or|and|not|neg"
