        ,"image": {
            "name": "cld.image"
            ,"begin": "(?i)image"
            ,"beginCaptures": {
                "0": {
                    "name": "constant.numeric"
                }
            }
            ,"end": "(?<=\")"
            ,"patterns": [
                {
                    "include": "#string"
                }
                ,{
                    "include": "#comment"
