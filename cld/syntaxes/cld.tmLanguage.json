                        {
                            "name": "constant.numeric"
                        }
                    }
                    ,"end": "(?i)[a-z$][a-z$_0-9]*"
                    ,"endCaptures": {
                        "0":
                        {
                            "name": "markup.italic"
                        }
                    }
                    ,"patterns": [
                        {
                            "include": "#comment"
                        }
                    ]
                }
                ,{
                    "include": "#parameter.clauses"
                }
            ]
        }
        ,"parameter.clauses":
        {
            "patterns": [
                {
                    "include": "#default"
                }
                ,{
                    "include": "#label"
                }
                ,{
                    "include": "#prompt"
                }
                ,{
                    "include": "#parameter.value"
                }
                ,{
                    "include": "#comment"
                }
            ]
        }
        ,"default": 
