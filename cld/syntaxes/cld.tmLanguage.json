        ,"value.default": {
            "name": "cld.value.default"
            ,"begin": "(?i)default"
            ,"beginCaptures": {
                "0": {
                    "name": "constant.character"
                }
            }
            ,"end": "(?<=\")"
            ,"patterns": [
