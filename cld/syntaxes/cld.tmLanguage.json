        ,"qualifier": {
            "name": "cld.qualifier"
            ,"begin": "(?i)(?=qualifier)"
            ,"end": "(?i)(?!\\G)(?=define|ident|module|routine|image|disallow|nodisallows|noparameters|noqualifiers|parameter|qualifier|synonym)"
            ,"patterns": [
                {
                    "begin": "\\G(?i)qualifier"
                    ,"beginCaptures": {
